{
    "name": "CSS for redis.io",
    "version": "1.3",
    "description": "Easy to read redis.io documents",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://redis.io/*"
            ],
            "css": [
                "redis.io.css"
            ]
        }
    ],
    "icons": {
        "128": "redis128.png"
    }
}